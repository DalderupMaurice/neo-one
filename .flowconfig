[version]
0.65.0

[ignore]
.*/node_modules/babel-plugin-flow-runtime/.*
.*/packages/.*/dist/es.js
.*/packages/.*/dist/es.js.map
.*/node_modules/protobufjs/src/bower.json
.*/node_modules/dataloader/index.js.flow
<PROJECT_ROOT>/migrate/.*

[include]

[libs]
decls/

[lints]
all=error
sketchy-null-bool=off
untyped-import=warn
unclear-type=off
unsafe-getters-setters=off

[options]
module.use_strict=true

esproposal.export_star_as=enable
experimental.const_params=true
